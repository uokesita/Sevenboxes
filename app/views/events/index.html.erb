<h2>This are my lastest events added</h2>
<div class="clear"></div>
<table id="events_table">
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td class="edit"><%= link_to 'Edit', edit_event_path(event) %></td>
    <td class="destroy"><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
